{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://sydney-event-api.onrender.com/api/:path*"
    },
    {
      "source": "/auth/:path*",
      "destination": "https://sydney-event-api.onrender.com/auth/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}